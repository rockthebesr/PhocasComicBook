extends layout

block content
    #login
        a(href = '/sign_in')
            img(src="images/signin.jpg", style='height:30px;width=50px;')

    #signup
        a(href = '/sign_up')
            img(src="images/signup.png", style='height:35px;width=50px;')

    #edit
        a(href = '/edit_comic')
            img(src="images/edit.png", style='height:40px;width=60px;')

    for urlandtitle in urlAndtitle
        li
            p #{urlandtitle[0]}
            a(href = '/comic_page')
                img(class="uploadedImage", src= '#{urlandtitle[1]}')
